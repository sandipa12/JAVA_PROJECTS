
public class ComputeArea {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String a = "Sandipa ";
		String b = "Pande";
		
		// Concatenate Array.
		String c = a + b;
		
		int d = c.length();
		
		int e = d;
		
		int f = e;
		// Print
		System.out.println(f);	

	}		

}
